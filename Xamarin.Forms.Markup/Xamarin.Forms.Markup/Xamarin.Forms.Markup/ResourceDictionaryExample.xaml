<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 x:Class="Xamarin.Forms.Markup.ResourceDictionaryExample">
	<!-- Causes error when loaded: Cannot assign property "Resources": type mismatch between "System.String" and "Xamarin.Forms.ResourceDictionary" -->
	<ContentPage.Resources>
		<x:String x:Key="KeyName">Hello World!</x:String>
	</ContentPage.Resources>
	<Label Text="{Binding MainText}" VerticalOptions="Center" HorizontalOptions="Center" />
</ContentPage>